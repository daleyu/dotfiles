{
    "python.pythonPath": "/usr/bin/python3",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
      "acnlin56.pbn.bnl.gov": "linux",
      "acnlingf.pbn.bnl.gov": "linux",
      "acnlin6a.pbn.bnl.gov": "linux",
      "clac.cs.columbia.edu": "linux",
      "osvm": "linux"
    },
    "git.autofetch": true,
    "editor.inlineSuggest.enabled": true,
    "git.ignoreMissingGitWarning": true,
    "application.shellEnvironmentResolutionTimeout": 20,
    "[dart]": {
      "editor.formatOnSave": true,
      "editor.formatOnType": true,
      "editor.rulers": [80],
      "editor.selectionHighlight": false,
      "editor.suggest.snippetsPreventQuickSuggestions": false,
      "editor.suggestSelection": "first",
      "editor.tabCompletion": "onlySnippets",
      "editor.wordBasedSuggestions": "off",
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
      }
    },
    "github.copilot.enable": {
      "*": true,
      "plaintext": false,
      "markdown": false,
      "scminput": false,
      "yaml": false,
      "dart": true,
      "go": false
    },
    "[python]": {
      "editor.formatOnType": true,
      "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "workbench.iconTheme": "catppuccin-mocha",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.minimap.autohide": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.scale": 2,
    "terminal.integrated.env.osx": {},
    "workbench.colorTheme": "Catppuccin Mocha",
    "redhat.telemetry.enabled": true,
    "editor.rulers": [80, 100],
    "rewrap.autoWrap.enabled": true,
    "clangd.detectExtensionConflicts": false,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "files.autoSave": "off",
    "[javascript][javascriptreact][typescript][typescriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "explicit"
      }
    },
    "eslint.options": {
      "extensions": [".js", ".jsx"]
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "[c]": {
      "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "githubPullRequests.createOnPublishBranch": "never",
    "git.openRepositoryInParentFolders": "always",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "[latex]": {
      "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "[go]": {
      "editor.defaultFormatter": "golang.go"
    },
    "terminal.integrated.fontFamily": "monospace",
    "git.confirmSync": false,
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["g", "H"],
        "commands": [{ "command": "editor.action.showDefinitionPreviewHover" }]
      },
      {
        "before": ["u"],
        "after": [],
        "commands": [
          {
            "command": "undo",
            "args": []
          }
        ]
      },
      {
        "before": ["<C-r>"],
        "after": [],
        "commands": [
          {
            "command": "redo",
            "args": []
          }
        ]
      }
    ],
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.statusBarColorControl": false,
    "makefile.configureOnOpen": true,
    "vim.vimrc.enable": true
  }
  